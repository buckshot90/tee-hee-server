{
  "env": "development",
  "port": 3000,
  "logDir": "./logs",
  "apiVersion": "1.0.0",
  "routes": {
    "1.0.0": {
      "/cards/": {
        "get": "cardsController.index",
        "post": "cardsController.create"
      },
      "/cards/:card": {
        "get": "cardsController.card",
        "put": "cardsController.update",
        "delete": "cardsController.remove"
      }
    }
  }
}